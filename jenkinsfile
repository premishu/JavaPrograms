pipeline {
    agent any 
    stages {
        stage('Compile stage') 
		{ 
            	steps 
		{
		withMaven(maven	: 'maven_3_5_0')
		sh 'mvn clean compile'
                }
        	}
        stage('Tessting Stage') 
		{ 
            	steps 
		{
		withMaven(maven	: 'maven_3_5_0')
		sh 'mvn test'
               	}
        	}
        stage('Deploy') 
		{ 
            steps {
                withMaven(maven	: 'maven_3_5_0')
		sh 'mvn deploy'
            }
        }
    }
}
